<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="softserver" namespace="/soft" extends="default">
		<action name="soft" class="softwareAction">
			<result name="soft_view">/WEB-INF/softserver/soft_view.jsp</result>
		</action>
		<action name="dir" class="ServiceDirAction">
			<result name="list_service_module">/WEB-INF/softserver/services_list.jsp</result>		
			<result name="view_service_module">/WEB-INF/softserver/services_view.jsp</result>
			<result name="dir_view">/WEB-INF/softserver/servicedir_view.jsp</result>
		</action>
		<action name="param" class="DirParamsAction">
			<result name="param_view">/WEB-INF/softserver/params_view.jsp</result>
		</action>
		<action name="server" class="serverAction">
			<result name="server_view">/WEB-INF/softserver/server_view.jsp</result>
		</action>
    </package>
</struts>
