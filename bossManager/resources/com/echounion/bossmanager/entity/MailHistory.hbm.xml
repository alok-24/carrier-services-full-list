<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.echounion.bossmanager.entity.MailHistory" table="msg_mail_history">
        <id name="id" >
            <column name="id" />
            <generator class="native" />
        </id>
       <property name="softId" column="sid"/>
       <property name="emailAddress" column="email_address"/>
       <property name="tplName" column="tpl_name"/>
       <property name="sendTime" column="send_time"/>
       <property name="errosNum" column="erros_num"/>
       <property name="lastTime" column="last_time"/>
       <property name="content" column="content"/>
       <property name="status" column="status"/>
       <property name="bcode" column="bcode"/>
	   <property name="btype" column="btype"/>
    </class>
</hibernate-mapping>
