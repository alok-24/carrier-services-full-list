<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.echounion.bossmanager.entity.EsbServiceDirParam" table="esb_params">
        <id name="id" >
            <column name="id" />
            <generator class="native" />
        </id>
       	<property name="name" column="name"/>
       	<property name="code" column="code"/>       	
       	<property name="typeId" column="type_id"/>
       	<property name="require"  column="is_require"/>
       	<property name="serviceDirId" column="service_dir_id"/>
       	<property name="remark" column="remark"/>
    </class>
</hibernate-mapping>
